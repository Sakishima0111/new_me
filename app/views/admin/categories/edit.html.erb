      <h3>カテゴリ編集</h3>

      <%= form_with model: @category, url: admin_category_path,method: :patch do |f| %>
        <td><%= f.label :"カテゴリ名", class: "mr-3" %></td>
        <td><%= f.text_field :name, class: "mr-5" %></td>
        <td><%= f.submit "変更を保存", class: "btn btn-primary" %></td>
      <% end %>